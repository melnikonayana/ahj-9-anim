{
  "extends": "airbnb-base",
  "env": {
    "es2022": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "import/prefer-default-export": [
      "off"
    ],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true}
    ],
    "import/extensions": [
      "error",
      "ignorePackages"
    ],
    "no-underscore-dangle": [
      "off"
    ],
    "class-methods-use-this": [
      "off"
    ],
    "no-plusplus": [
      "off"
    ],
    "no-tabs": "off",
    "indent": ["error", 2]
  }
}